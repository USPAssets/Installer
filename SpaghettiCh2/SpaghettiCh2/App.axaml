<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:USPInstaller"
             x:Class="USPInstaller.App"
			 RequestedThemeVariant="Dark">

	<Application.DataTemplates>
		<local:ViewLocator/>
	</Application.DataTemplates>
	
    <Application.Styles>
		<FluentTheme />
    </Application.Styles>
	<Application.Resources>
		<FontFamily x:Key="DeterminationFont">avares://USPInstaller/Assets/determination.ttf#Determination Sans</FontFamily>
	</Application.Resources>
</Application>
